{
  "name": "dogfight_arena",
  "version": "1.0.0",
  "description": "An aerial warfare game/experiment",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefanuros/dogfight_arena.git"
  },
  "author": "",
