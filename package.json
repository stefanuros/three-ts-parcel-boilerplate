    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "husky": "^7.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  }
}
