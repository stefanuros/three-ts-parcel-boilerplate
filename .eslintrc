{
    "env": {
        "browser": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["*.html"],
    "rules": {
        "require-jsdoc": "off",
        "semi": "off",
        "linebreak-style": "off"
    }
}
